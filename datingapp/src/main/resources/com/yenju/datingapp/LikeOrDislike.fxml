      <Label fx:id="lb_name" layoutX="210.0" layoutY="200.0" prefHeight="15.0" prefWidth="138.0" text="Name" />
      <Button fx:id="button_like" layoutX="143.0" layoutY="270.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="109.0" text="Like">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="button_dislike" layoutX="307.0" layoutY="270.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="109.0" text="Dislike">
